{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "coverage": "turbo run coverage",
    "cs": "changeset",
    "depcheck": "turbo run depcheck",
    "dev": "turbo run dev --parallel",
    "gca": "pnpm --filter=google-cloud-api",
    "postinstall": "husky install",
    "iso": "pnpm --filter=iso-error",
    "iso-gca": "pnpm --filter=iso-error-google-cloud-api",
    "web": "pnpm --filter=iso-error-web",
    "lint": "turbo run lint",
    "openapi:lint": "turbo run openapi:lint",
    "nuke": "turbo run nuke && rimraf node_modules",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "release": "changeset publish",
    "size": "turbo run size",
    "test": "turbo run test",
    "verify": "turbo run lint build coverage depcheck size",
    "version": "changeset version",
    "watch": "jest --watch"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "eslint": "^8.27.0",
    "eslint-plugin-harmony": "^7.1.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-progress-tracker": "^3.0.4",
    "jest-validate": "^29.3.1",
    "jest-watch-repeat": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.2.1",
    "npm-check-updates": "^16.4.1",
    "pinst": "^3.0.0",
    "rimraf": "^3.0.2",
    "turbo": "^1.6.3",
    "typescript": "^4.9.3"
  },
  "packageManager": "pnpm@7.22.0"
}
