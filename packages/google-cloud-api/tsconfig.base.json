{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "lib": [
      "ES2015",
      "ESNext.Promise"
    ],
    "moduleResolution": "node",
    "newLine": "LF",
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "paths": {
      "iso-error": [
        "../iso-error/ts"
      ]
    },
    "removeComments": false,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "useDefineForClassFields": true
  },
  "include": [
    "typings"
  ],
  "files": [
    "ts/index.ts"
  ]
}
