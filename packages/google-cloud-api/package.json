{
  "name": "google-cloud-api",
  "version": "3.2.4",
  "description": "API utility library that follows Google Cloud API design",
  "homepage": "https://github.com/unional/iso-error/tree/main/packages/google-cloud-api",
  "bugs": {
    "url": "https://github.com/unional/iso-error/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unional/iso-error.git"
  },
  "license": "MIT",
  "author": {
    "name": "Homa Wong (unional)",
    "email": "homawong@gmail.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "./cjs/index.js",
  "types": "./esm/index.d.ts",
  "files": [
    "cjs",
    "esm",
    "ts",
    "!ts/**/*.spec.ts",
    "!ts/test-utils",
    "open_api"
  ],
  "scripts": {
    "build": "npm-run-all ts:clean -p ts:build:cjs ts:build:esm",
    "clean": "rimraf .nyc_output .tsbuildinfo coverage cjs esm lib libm || true",
    "coverage": "jest --coverage",
    "depcheck": "depcheck",
    "lint": "cross-env TIMING=1 eslint --ext=js,ts .",
    "openapi:validate": "run-p openapi:validate:google-clould-api openapi:validate:protobuf openapi:validate:rpc",
    "openapi:validate:google-clould-api": "swagger-cli validate open_api/google-cloud-api.yml",
    "openapi:validate:protobuf": "swagger-cli validate open_api/google.protobuf.yml",
    "openapi:validate:rpc": "swagger-cli validate open_api/google.rpc.yml",
    "nuke": "run-s clean nuke:local",
    "nuke:local": "rimraf node_modules || true",
    "size": "size-limit",
    "test": "jest",
    "ts:build:cjs": "tsc -p tsconfig.cjs.json",
    "ts:build:esm": "tsc -p tsconfig.esm.json",
    "ts:clean": "rimraf .tsbuildinfo || true",
    "ts:test": "tsc",
    "verify": "npm-run-all -p build lint coverage depcheck openapi:validate -s size",
    "watch": "jest --watch"
  },
  "dependencies": {
    "iso-error": "workspace:^",
    "type-plus": "^4.11.1"
  },
  "devDependencies": {
    "@apidevtools/swagger-cli": "^4.0.4",
    "@size-limit/file": "^8.0.0",
    "@size-limit/webpack": "^8.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "assertron": "^10.0.0",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.21.0",
    "eslint-plugin-harmony": "^7.1.0",
    "jest": "^29.0.0",
    "jest-validate": "^29.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "jest-watch-typeahead": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "satisfier": "^5.2.2",
    "size-limit": "^8.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">= 8"
  }
}
