{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "cache": false
    },
    "coverage": {
      "dependsOn": [
        "^build"
      ]
    },
    "depcheck": {
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^build"
      ]
    },
    "openapi:lint": {},
    "nuke": {
      "dependsOn": [
        "clean"
      ],
      "cache": false
    },
    "size": {
      "dependsOn": [
        "build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "verify": {
    },
    "watch": {
      "dependsOn": [
        "^build"
      ]
    }
  }
}
